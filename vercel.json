{
    "builds": [
        {
            "src": "vercel_build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "data-analyst-roadmap/_build/html"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}